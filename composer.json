{
    "name": "phergie/phergie-irc-plugin-react-twitter",
    "description": "Phergie plugin for displaying data from tweets",
    "keywords": [ "phergie", "irc", "bot", "plugin", "twitter" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/phergie/phergie-irc-plugin-react-twitter/issues",
        "source": "https://github.com/phergie/phergie-irc-plugin-react-twitter"
    },
    "minimum-stability": "dev",
    "require": {
        "phergie/phergie-irc-bot-react": "dev-master",
        "guzzlehttp/guzzle": "4.*",
        "guzzlehttp/oauth-subscriber": "0.1.*",
        "wyrihaximus/phergie-url": "dev-master",
        "wyrihaximus/react-guzzle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phake/phake": "2.0.0-beta2"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Irc\\Plugin\\React\\Twitter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Irc\\Tests\\Plugin\\React\\Twitter\\": "tests"
        }
    }
}
